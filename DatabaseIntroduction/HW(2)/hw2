2-(1).

create table publisher 

(

    pub_code char(5) primary key,

    pname varchar2(20) not null,

    address varchar2(30),

    phone varchar2(13),

    est_date DATE

);

 

create table book

(

    isbn char(13) primary key,

    title varchar2(50) not null,

    author varchar2(20) not null,

    publisher char(5) references publisher(pub_code),

    price int

); 

-------------------------<스크립트 출력>----------------------------------------

Table PUBLISHER이(가) 생성되었습니다.

 

 

Table BOOK이(가) 생성되었습니다.

 

 

 

 

2-(2).

insert into publisher values ('21188', '민음사', '서울시 강남구', '02-515-2000', TO_DATE('1978-03-20', 'YYYY-MM-DD'));

insert into publisher (pub_code, pname, address, phone) values ('20881', '문학동네', '경기도 파주시', '031-955-8888');

insert into publisher (pub_code, pname, address) values ('10581', '창비', '경기도 파주시');

insert into publisher values ('19009', '위즈덤하우스', '서울시 마포구', '02-2179-5600', TO_DATE('2008-04-01', 'YYYY-MM-DD'));

insert into publisher values ('04003', '길벗', '서울시 마포구', '02-332-0931', TO_DATE('1991-12-24', 'YYYY-MM-DD'));

select * from publisher;

 

insert into book values ('323-572493-01', '안나 카레리나', '톨스토이', '21188', 12000);

insert into book values ('787-123456-02', '전쟁과 평화', '톨스토이', '21188', 9000);

insert into book values ('983-223344-03', '오만과 편견', '제인 오스틴', '20881', 11000);

insert into book values ('978-893645-70', '안녕을 말할 땐 천천히', '모니크 폴락', '19009', 12000);

insert into book values ('979-116220-62', '데미안', '헤르만 헤세', '19009', 10000);

select * from book;

 

